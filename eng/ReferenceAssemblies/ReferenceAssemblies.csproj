<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
        <TargetFramework>netcoreapp3.0</TargetFramework>
    </PropertyGroup>

    <ItemGroup>
        <Reference Include="@(ProjectReferenceProvider)" />
    </ItemGroup>

    <Target Name="GenerateReferenceSources" DependsOnTargets="ResolveAssemblyReferences">
        <MSBuild
            Projects="%(ReferencePath.ProjectPath)"
            Condition="'%(ReferencePath.ProjectPath)' != ''"
            Targets="GenerateReferenceSource"
            Properties="%(ReferencePath.SetTargetFramework)"
            BuildInParallel="true"/>
    </Target>

</Project>